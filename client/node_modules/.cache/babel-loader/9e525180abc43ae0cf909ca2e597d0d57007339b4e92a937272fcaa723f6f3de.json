{"ast":null,"code":"import axios from 'axios';\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: 'Login',\n  data() {\n    return {\n      email: '',\n      password: ''\n    };\n  },\n  methods: {\n    login(e) {\n      e.preventDefault();\n      axios.post('http://localhost:5000/api/authenticate', {\n        email: this.email,\n        password: this.password\n      }).then(res => {\n        if (res.data.success) {\n          this.$emit('auth', res.data.token);\n        } else {\n          console.log(res.data.msg);\n        }\n      }).catch(err => console.log(err));\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","methods","login","e","preventDefault","post","then","res","success","$emit","token","console","log","msg","catch","err"],"sources":["/home/stagardazi/Desktop/Self-Motivation-Drama/MEVNApp/client/src/components/Login.vue"],"sourcesContent":["\n<template>\n    <div>\n      <h2>Login</h2>\n      <form @submit=\"login\">\n        <label>Email: </label>\n        <input v-model=\"email\" type=\"text\">\n        <label>Password: </label>\n        <input v-model=\"password\" type=\"password\">\n        <button type=\"submit\">Login</button>\n      </form>\n    </div>\n  </template>\n  \n  <script>\n  import axios from 'axios';\n  \n  export default {\n    \n    // eslint-disable-next-line vue/multi-word-component-names\n    name: 'Login',\n    data() {\n      return {\n        email: '',\n        password: ''\n      };\n    },\n    methods: {\n      login(e) {\n        e.preventDefault();\n        axios.post('http://localhost:5000/api/authenticate', {\n          email: this.email,\n          password: this.password\n        })\n        .then(res => {\n          if (res.data.success) {\n            this.$emit('auth', res.data.token);\n          } else {\n            console.log(res.data.msg);\n          }\n        })\n        .catch(err => console.log(err));\n      }\n    }\n  };\n  </script>\n  "],"mappings":"AAeE,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EAEb;EACAC,IAAI,EAAE,OAAO;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,KAAKA,CAACC,CAAC,EAAE;MACPA,CAAC,CAACC,cAAc,CAAC,CAAC;MAClBR,KAAK,CAACS,IAAI,CAAC,wCAAwC,EAAE;QACnDN,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC,EACAM,IAAI,CAACC,GAAE,IAAK;QACX,IAAIA,GAAG,CAACT,IAAI,CAACU,OAAO,EAAE;UACpB,IAAI,CAACC,KAAK,CAAC,MAAM,EAAEF,GAAG,CAACT,IAAI,CAACY,KAAK,CAAC;QACpC,OAAO;UACLC,OAAO,CAACC,GAAG,CAACL,GAAG,CAACT,IAAI,CAACe,GAAG,CAAC;QAC3B;MACF,CAAC,EACAC,KAAK,CAACC,GAAE,IAAKJ,OAAO,CAACC,GAAG,CAACG,GAAG,CAAC,CAAC;IACjC;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}